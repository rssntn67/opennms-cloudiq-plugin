<events xmlns="http://xmlns.opennms.org/xsd/eventconf">
    <event>
        <uei>uei.opennms.org/cloudiq/clearEvent</uei>
        <event-label>OpenNMS CloudIQ Plugin: Clear event</event-label>
        <descr>Clear Event: %parm[all]%</descr>
        <logmsg dest="logndisplay">OpenNMS CloudIQ Plugin: Clear Event: %parm[id]% system %parm[system_name]%</logmsg>
        <severity>Normal</severity>
        <alarm-data reduction-key="%parm[id]%" alarm-type="2" clear-key="%parm[id]%"/>
    </event>
    <event>
        <uei>uei.opennms.org/cloudiq/raiseEvent</uei>
        <event-label>OpenNMS CloudIQ Plugin: Raise event</event-label>
        <descr>Raise Event: %parm[all]%</descr>
        <logmsg dest="logndisplay">OpenNMS CloudIQ Plugin: Raise Event: %parm[id]% system %parm[system_name]%</logmsg>
        <severity>Critical</severity>
        <alarm-data reduction-key="%parm[id]%" alarm-type="1" />
    </event>
</events>